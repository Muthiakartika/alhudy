/* Media Querry */

@media (min-width: 1024px) {

  #hero {
    background-attachment: fixed;
  }
}
@media (max-width: 768px) {
  .about .content {
    padding-right: 12px;
  }
  #hero h1 {
    font-size: 28px;
    line-height: 36px;
  }
  #hero h2 {
    font-size: 20px;
    line-height: 24px;
  }
}
  